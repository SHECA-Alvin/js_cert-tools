{
  "name": "sheca-tool-web",
  "version": "1.0.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "cross-env NODE_OPTIONS= umi build --mode production",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "npm run start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier && npm run tsc",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "openapi": "umi openapi",
    "playwright": "playwright install && playwright test",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev umi dev --port=8002",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev NODE_OPTIONS=--openssl-legacy-provider umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit",
    "docker:build": "docker build -f ./Dockerfile -t registry.cn-qingdao.aliyuncs.com/cert-center/web-cert-tool:sheca .",
    "docker:push": "cross-env npm run build && npm run docker:build && docker push registry.cn-qingdao.aliyuncs.com/cert-center/web-cert-tool:sheca"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@ant-design/pro-card": "^1.18.16",
    "@ant-design/pro-descriptions": "^1.9.0",
    "@ant-design/pro-form": "^1.43.0",
    "@ant-design/pro-layout": "^6.26.0",
    "@ant-design/pro-table": "^2.56.0",
    "@lapo/asn1js": "^2.1.0",
    "@umijs/route-utils": "^2.0.3",
    "antd": "^4.18.8",
    "asn1.js": "^5.4.1",
    "asn1js": "^3.0.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "classnames": "^2.2.6",
    "crypto-js": "^4.2.0",
    "file-saver": "^2.0.5",
    "gmsm-sm2js": "^0.3.3",
    "jsrsasign": "^10.8.6",
    "jszip": "^3.10.1",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "node-forge": "^1.3.1",
    "omit.js": "^2.0.2",
    "pkijs": "^3.2.5",
    "rc-menu": "^9.0.13",
    "rc-util": "^5.14.0",
    "react": "^17.0.0",
    "react-copy-to-clipboard": "^5.0.4",
    "react-csv": "^2.2.2",
    "react-dev-inspector": "^1.1.1",
    "react-dom": "^17.0.0",
    "react-helmet-async": "^1.0.4",
    "umi": "^3.5.0",
    "umi-serve": "^1.9.10",
    "use-state-with-callback": "^2.0.3"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.0.2",
    "@playwright/test": "^1.16.3",
    "@types/express": "^4.17.0",
    "@types/file-saver": "^2.0.4",
    "@types/history": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/openapi": "^1.3.0",
    "@umijs/plugin-blocks": "^2.0.5",
    "@umijs/plugin-esbuild": "^1.0.1",
    "@umijs/plugin-openapi": "^1.2.0",
    "@umijs/preset-ant-design-pro": "^1.2.0",
    "@umijs/preset-dumi": "^1.1.7",
    "@umijs/preset-react": "^2.1.1",
    "@umijs/yorkie": "^2.0.3",
    "carlo": "^0.9.46",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.1.1",
    "detect-installer": "^1.0.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.9.0",
    "express": "^4.17.3",
    "gh-pages": "^3.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^12.3.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.3.2",
    "puppeteer-core": "^13.3.2",
    "stylelint": "^14.5.1",
    "swagger-ui-react": "^3.52.3",
    "typescript": "^4.2.2",
    "webpack-subresource-integrity": "^5.2.0-rc.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
